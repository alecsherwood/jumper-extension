{
  "manifest_version": 2,
  "name": "Jumper Extension",
  "description": "Chrome extension for controling browser via MIDI Signals.",
  "version": "1.0.0",
  "actions": {
    "default_popup": "index.html",
    "default_title": "Jumper Extension Settings"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "background": [
    {
      "scripts  ": ["background.js"]
    }
  ]
}